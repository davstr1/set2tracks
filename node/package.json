{
  "name": "set2tracks-node",
  "version": "1.0.0",
  "description": "Set2Tracks - DJ Set Tracklist Generator (Node.js/TypeScript version)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:introspect": "prisma db pull",
    "prisma:seed": "ts-node src/utils/seed.ts",
    "tailwind:build": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css",
    "tailwind:watch": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --watch",
    "jobs:dev": "ts-node-dev src/jobs/worker.ts",
    "jobs:prod": "node dist/jobs/worker.js"
  },
  "keywords": [
    "dj",
    "tracklist",
    "music",
    "shazam",
    "spotify",
    "youtube"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.3.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "helmet": "^8.1.0",
    "i18next": "^23.7.6",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.5.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-shazam": "^1.1.2",
    "nodemailer": "^6.9.7",
    "nunjucks": "^3.2.4",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "spotify-web-api-node": "^5.0.2",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4",
    "youtube-dl-exec": "^3.0.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bull": "^4.10.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/nunjucks": "^3.2.6",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-local": "^1.0.38",
    "@types/spotify-web-api-node": "^5.0.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
